.iconfont {
  width: 10vw;
  font-family: "iconfont" !important;
  font-size: 10vw;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  padding-right: 2vw;
  position: relative;
  float: inline-end;
  display: flex;
  align-items: center;
  justify-content: center;
}
.icon-xingkong-copy:before {
  content: "\e745";
}
.icon-jiahao:before {
  content: "\e728";
}
/*主要的样式*/
.plus-css{
  display: flex;
}
.event-top{
  display: flex;
}
/* 事件上边栏的样式 */
.event-top view{
  position: relative;
  width: 100%;
  font-size:x-large;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  left: 6vw;
}
/*  */
.event-top{
  /* background-color: aquamarine; */
  width: 100%;
  height:10vw ;
  display: flex;
  border-bottom:3px solid  #9393932c;
  background-color:white;
}
/* 圆形图片 */
.cricle{
  width: 22px;
  padding-right: 1vw;
}
.text-dir{
  display: flex;
  width: 80vw;
  font-size: large;
  margin-left: 10px;
}
.events{
  padding: 5px 10px;
  border-radius: 1em;
  display: flex;
  direction: row;
  align-items: center;
  margin: 13rpx;
  background-color: white;
  width: 90%;
  height: 35px;
}
.event-continer{
  background-color: #9393932c;
  width: 100%;
  display: flex;
  direction: column;
  flex-wrap: wrap;
  align-items: center;
  justify-content: space-between;
  padding-bottom: 10px;
  padding-top: 7px;
}

