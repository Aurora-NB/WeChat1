.individuals{
  padding-top: 50rpx;
  margin: 0;
  width: 100%;
  height: 1000rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.user{
  background-color: #ffffff;
  width: 90%;
  height: 100px;
  border-radius: 1em;
  display: flex;
  align-items: center;
  padding-left: 20rpx;

}
.userhead{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  background-color: rgb(187, 186, 186);
  border: 2px solid black; 
  margin-right: 30rpx;
}
.detail{
  height: 60%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.other-options{
  margin-top: 100rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
  padding-right: 40rpx;
}
.options{
  width: 90%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx;
  padding-left: 50rpx;
}
.arrow{
  height: 30rpx;
  width: 30rpx;
}