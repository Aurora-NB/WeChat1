/*此处是标签的图标样式 */
@font-face {
  font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_1789111_dwfbszgyisb.eot?t=1588171437722');
  /* IE9 */
  src: url('//at.alicdn.com/t/font_1789111_dwfbszgyisb.eot?t=1588171437722#iefix') format('embedded-opentype'),
    /* IE6-IE8 */
    url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
    url('//at.alicdn.com/t/font_1789111_dwfbszgyisb.woff?t=1588171437722') format('woff'),
    url('//at.alicdn.com/t/font_1789111_dwfbszgyisb.ttf?t=1588171437722') format('truetype'),
    /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
    url('//at.alicdn.com/t/font_1789111_dwfbszgyisb.svg?t=1588171437722#iconfont') format('svg');
  /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 6vw;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-shoucang-:before {
  content: "\e650";
}

.icon-shoucang--copy:before {
  content: "\e651";
}

.icon-ziyuan-copy:before {
  content: "\e652";
}

.icon-ziyuan:before {
  content: "\e615";
}

.icon-class {
  width: 6vw;
  display: inline-block;
}

/* 事件的输入 */
.text-input {
  width: 96vw;
  height: 30vw;
  display: flex;
  font-weight: 400;
  /* background-color:olivedrab; */

}

.text-view {
  display: flex;
  padding-top: 5vw;
  align-items: center;
  justify-content: center;
}

/* 提交按钮的样式 */
.submit-button {
  width: 15vw;
  height: 10vw;
  float: right;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  text-align: center;
  color: rgb(104, 191, 123);
  border-radius: 1em;
  background-color: white;
  box-shadow: 1vw 1vw 1vw 1vw rgba(85, 85, 85, 0.10);
  position: relative;
  left: -4vw;

}

/* 标签摁下回车符以后的样式 */
.biaoqian-input {
  width: 18vw;
  padding-right: 2vw;
  font-weight: 600;
  display: inline-block;

}

.biaoqian-before {
  height: 6vw;
  width: 96%;
  padding-left: 1%;
  padding-right: 2%;
  padding-top: 1.5vw;
}

/* 标题输入框的样式 */
.header-input {
  min-height: 8vw;
  font-size: 6vw;
  font-weight: 900;
  width: 96vw;
  padding-left: 3vw;
  padding-right: 2vw;
  padding-bottom: 1vw;
}

/* 图片下载的样式 */
.download {
  padding-left: 25vw;
  width: 50vw;
}

.header-view {
  border-bottom: 0.1vw black solid;

  padding-bottom: 3vw;
}

