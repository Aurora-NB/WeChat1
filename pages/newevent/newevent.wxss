.biaoqianinput{
  width: 32vw;
  font-weight: 500;
  min-height: 6vw;
  padding-left:1.5vw;
}
.biaoqianview{
  display: inline-block;
  width: 32vw;
}
/* 删除标签的样式 */
.deletebiaoqian{
  display: inline-block;

}
